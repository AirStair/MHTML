{
    "manifest_version": 3,
    "minimum_chrome_version": "122.0.6261.137",
    "name": "MHTML",
    "version": "1.0.0",
    "description": "Save as MHTML",
    "action": {
        "default_icon": {
            "24": "icons/icon24.png"
        }
    },
    "icons": {
        "96": "icons/icon96.png",
        "48": "icons/icon48.png",
        "24": "icons/icon24.png"
    },
    "background": {
        "service_worker": "service_worker.js"
    },
    "permissions": [
        "pageCapture",
        "activeTab",
        "downloads"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}